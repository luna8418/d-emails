{
  "name": "d-emails",
  "version": "1.0.0",
  "description": "D. Email Services",
  "main": "dist/main.js",
  "scripts": {
    "clean": "rm -f .tsbuildinfo && rm -rf dist/*",
    "copy": "cp src/swagger.yaml dist/swagger.yaml",
    "compile": "tsc -p tsconfig.json",
    "dev": "npm run clean && npm run copy && npm run compile && nodemon dist/main.js",
    "dev:watch": "tsc --watch & nodemon dist/main.js",
    "jest": "npm run jest:unit && npm run jest:int",
    "jest:unit": "jest --passWithNoTests '^((?!\\.int\\.spec\\.ts).)*$'",
    "jest:int": "jest --passWithNoTests --runInBand '.*\\.int\\.spec\\.ts'",
    "jest:coverage": "jest --passWithNoTests '^((?!\\.int\\.spec\\.ts).)*$' --coverage",
    "lint": "eslint ./src/ --ext .ts",
    "lint:fix": "eslint ./src/ --ext .ts --fix"
  },
  "author": "Eric Yao",
  "license": "UNLICENSED",
  "dependencies": {
    "@sendgrid/mail": "^6.4.0",
    "body-parser": "^1.19.0",
    "bunyan": "^1.8.12",
    "cls-hooked": "^4.2.2",
    "convict": "^5.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.2",
    "express-ajv-swagger-validation": "^0.9.0",
    "express-bunyan-logger": "^1.3.3",
    "express-healthcheck": "^0.1.0",
    "lodash": "^4.17.21",
    "swagger-ui-express": "^4.1.2",
    "uuidv4": "^6.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "@types/chance": "^1.0.4",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.16.1",
    "@types/jest": "^24.0.13",
    "@types/lodash": "^4.14.118",
    "@types/nock": "^9.3.1",
    "@types/node": "^11.13.10",
    "@types/swagger-ui-express": "^4.1.0",
    "@types/yamljs": "^0.2.30",
    "chance": "^1.0.18",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "jest": "^24.8.0",
    "nock": "^10.0.6",
    "node-mocks-http": "^1.7.3",
    "nodemon": "^1.18.10",
    "supertest": "^3.4.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.1.0",
    "typescript": "^3.4.5"
  }
}